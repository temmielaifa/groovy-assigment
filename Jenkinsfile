#!/bin/Groovy
pipeline {
    agent any
    stages {
        stage('What is your name') {
           
            steps {
                echo 'My name is Abbey cheche'
                
            }
        }
        stage('Check date') {
           
            steps {
                sh 'date'
               
            }
        }
        stage('Check calendar') {
           
            steps {
                sh 'touch binto.sh'
               
            }
        }
         stage('Class') {
           
            steps {
                echo 'i love this class >> niko.txt'
               
            }
        }
         stage('situation') {
           
            steps {
                echo 'nigerian network cut cheche off'
               
            }
        }
         stage('honourable cheche') {
           
            steps {
                sh 'touch alliance.sh'
               
            }
        } 
         stage('nigeria') {
           
            steps {
                sh 'touch apc.sh'
               
            }
        }
         stage('my homework') {
           
            steps {
                echo 'trying my assignment out >> kilanko.txt'
               
            }
        }
         stage('apc kosofe') {
           
            steps {
                echo 'abbey cheche is the incoming rep for kosofe >> kosofe.txt'
               
            }
        }
    }
}
